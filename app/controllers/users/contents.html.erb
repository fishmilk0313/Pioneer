<div class ="container">
	<div class ="row">
		<h1 class ="col-xs-8 text-center">category検索/<small><%= @posts.count %></small></h1>
		<%= render 'users/posts/post_index', {posts: @posts } %>
	</div>
	<div class ="col-xs-4">
		<h1>index</h1>
		<%= @categories each do |category|  %>
		<%= link_to contents_users_post_path (category.id) do %>
		  <%= category.name %>
		<% end %>
	</div>
</div>
</div>
